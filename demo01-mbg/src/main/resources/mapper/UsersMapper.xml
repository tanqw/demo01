<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tan.mapper.UsersMapper">
    <select id="getUserRoleRelation" resultMap="com.gitee.chengcheng.myblogs.mapper.RoleMapper.BaseResultMap">
            select r.* from users_role_relation ur LEFT JOIN role r on ur.role_id=r.id
            where ur.users_id=#{usersId}
    </select>
</mapper>